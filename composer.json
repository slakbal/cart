{
    "name": "slakbal/cart",
    "description": "Shoppingcart fork for Laravel that allows zero (0) quantity for an item in the cart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Gloudemans",
            "email": "info@robgloudemans.nl"
        }
    ],
    "require": {
        "laravel/framework": "5.5.*"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Slakbal\\Cart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slakbal\\Cart\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Slakbal\\Cart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "Slakbal\\Cart\\Facades\\Cart"
            }
        }
    },
    "minimum-stability": "stable"
}
